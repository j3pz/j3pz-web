{
    "include": ["./src/**/*", "./gatsby-*.js"],
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "es6",
        "module": "esnext",
        "lib": ["dom", "es2017"],
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmit": true,
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "skipLibCheck": true
    }
}
